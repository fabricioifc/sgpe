<%- model = Curso -%>
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">
      <%= titulo_painel(model) %>
    </h3>
  </div>
  <div class="panel-body">
    <div id="datatable">
      <%= render 'cursos' %>
    </div>
    <div id="paginator">
      <%= paginate @cursos, :remote => true %>
    </div>
  </div>
  <div class="panel-footer">
    <%= link_to_new model, new_curso_path %>
  </div>
</div>
